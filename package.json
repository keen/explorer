{
  "name": "keen-explorer",
  "description": "Data Explorer by Keen IO",
  "version": "6.0.28",
  "license": "MIT",
  "bucket-name": "keen-js",
  "main": "dist/keen-explorer.umd.js",
  "browser": "dist/keen-explorer.umd.js",
  "style": "dist/keen-explorer.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "concurrently --kill-others \"NODE_ENV=development webpack-dev-server\" \"npm run postcss-watch\"",
    "postcss-watch": "node_modules/postcss-cli/bin/postcss lib/css/index.css -o test/demo/keen-explorer.css --watch --config postcss.config.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "build": "NODE_ENV=production webpack -p && npm run build:bundle && npm run build:css && npm run build:css:min",
    "build:bundle": "NODE_ENV=production BUNDLE=1 webpack -p && NODE_ENV=production OPTIMIZE_MINIMIZE=1 BUNDLE=1 webpack -p ",
    "build:css": "node_modules/postcss-cli/bin/postcss lib/css/index.css -o dist/keen-explorer.css --config postcss.config.js",
    "build:css:min": "OPTIMIZE_MINIMIZE=1 node_modules/postcss-cli/bin/postcss lib/css/index.css -o dist/keen-explorer.min.css --config postcss.config.js",
    "version": "npm run build && git add .",
    "lint": "eslint lib/**/*.js --quiet",
    "circular": "madge --circular ./lib/**/*",
    "release": "standard-version",
    "commit": "npx git-cz"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/jestSetup.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "classnames": "2.1.2",
    "csv-parse": "^4.8.7",
    "csv-stringify": "^5.3.6",
    "json-stable-stringify": "1.0.0",
    "keen-analysis": "^3.4.2",
    "keen-dataviz": "^3.13.6",
    "keen-tracking": "^4.4.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "qs": "2.2.3",
    "rc-time-picker": "^3.6.2",
    "react": "^16.8.6",
    "react-dates": "^19.0.2",
    "react-dom": "^16.7.0",
    "react-highlight": "^0.9.0",
    "react-json-view": "^1.19.1",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.0",
    "react-select": "^3.0.4",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-saga": "^1.0.0",
    "string": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^8.2.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-prismjs": "^1.0.2",
    "commitizen": "^4.0.3",
    "concurrently": "^3.5.1",
    "connect-history-api-fallback": "0.0.5",
    "css-loader": "^2.1.0",
    "cssnano": "^3.10.0",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^2.0.1",
    "git-cz": "^3.3.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "jest": "^22.4.3",
    "jest-environment-jsdom-c3": "^2.0.0",
    "jest-fetch-mock": "^2.1.2",
    "nock": "^9.2.6",
    "postcss": "^6.0.23",
    "postcss-cli": "^5.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-css-variables": "^0.8.1",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^2.1.3",
    "precss": "^3.1.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "redux-devtools": "^3.5.0",
    "regenerator-runtime": "^0.11.1",
    "replace-in-file": "^3.4.0",
    "run-sequence": "~1.0.1",
    "standard-version": "^7.0.1",
    "stringify": "~3.0.0",
    "style-loader": "^0.20.3",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.3.1",
    "xhr-mock": "^2.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:keen/explorer.git"
  },
  "bugs": "https://github.com/keen/explorer/issues",
  "author": "Keen IO <team@keen.io> (https://keen.io/)",
  "homepage": "https://keen.io",
  "keywords": [
    "Keen.io",
    "self service analytics",
    "Keen charts",
    "Data explorer",
    "Data insights"
  ]
}
