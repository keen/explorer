{
  "name": "@keen.io/explorer",
  "description": "Data Explorer by Keen IO",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/keen-explorer.umd.js",
  "browser": "dist/keen-explorer.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "concurrently --kill-others \"NODE_ENV=development webpack-dev-server\"",
    "test": "jest",
    "test:ci": "jest -w 1",
    "test:watch": "NODE_ENV=test jest --watch",
    "build": "yarn build:legacy && yarn build:modern",
    "build:legacy": "NODE_ENV=production webpack --config webpack.config.prod.js --env.TARGET_LEGACY_BROWSERS=true -p",
    "build:modern": "NODE_ENV=production webpack --config webpack.config.prod.js --env.TARGET_LEGACY_BROWSERS=false -p",
    "check-bundle": "NODE_ENV=production webpack --config webpack.config.prod.js --env.TARGET_LEGACY_BROWSERS=false --env.ANALYZE_BUNDLE=true -p",
    "version": "npm run build && git add .",
    "lint": "eslint --quiet --ext .ts --ext .tsx lib/",
    "circular": "madge --circular ./lib/**/*",
    "prettier": "prettier --write 'lib/**/*.{js,ts,tsx,json}'",
    "release": "standard-version",
    "commit": "npx git-cz"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn prettier",
      "eslint --fix",
      "git add *"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@keen.io/colors": "^1.1.0",
    "@keen.io/dataviz": "^2.1.14",
    "@keen.io/forms": "^2.0.2",
    "@keen.io/icons": "^1.2.1",
    "@keen.io/parser": "^2.1.1",
    "@keen.io/pubsub": "^1.1.0",
    "@keen.io/query-creator": "^1.0.8",
    "@keen.io/react-hooks": "^1.0.1",
    "@keen.io/toast-notifications": "^1.1.2",
    "@keen.io/ui-core": "^2.5.16",
    "@keen.io/widget-picker": "^1.2.19",
    "camelcase-keys": "^6.2.2",
    "core-js": "^3.6.5",
    "dom-to-image": "^2.6.0",
    "file-saver": "^2.0.2",
    "framer-motion": "^1.11.0",
    "http-status-codes": "^1.4.0",
    "i18next": "^19.8.2",
    "i18next-http-backend": "^1.0.21",
    "keen-analysis": "^3.4.2",
    "keen-dataviz": "^3.13.6",
    "keen-tracking": "^4.4.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.31",
    "polished": "^3.4.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-i18next": "^11.7.3",
    "react-json-inspector": "^7.1.1",
    "react-redux": "^7.2.0",
    "react-syntax-highlighter": "^15.1.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-saga": "^1.0.0",
    "shallowequal": "^1.1.0",
    "string": "^2.0.1",
    "styled-components": "^5.1.0",
    "styled-system": "5.1.5",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
