  "license": "MIT",
  "main": "dist/explorer.modern.min.js",
  "browser": "dist/explorer.modern.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "concurrently --kill-others \"NODE_ENV=development webpack serve --mode development --env development --config webpack.config.dev.js\"",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "test": "jest",
    "test:ci": "jest -w 1",
    "test:watch": "NODE_ENV=test jest --watch",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js --env --progress --mode production --progress",
    "check-bundle": "NODE_ENV=production webpack --config webpack.config.prod.js --env ANALYZE_BUNDLE=true --progress",
    "version": "npm run build && git add .",
    "lint": "eslint --quiet --ext .ts --ext .tsx src/",
    "circular": "madge --circular ./src/**/*",
    "prettier": "prettier --write '{src,cypress}/**/*.{js,ts,tsx,json}'",
    "release": "standard-version",
    "commit": "npx git-cz"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/integration",
    "nonGlobalStepDefinitions": false
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn prettier",
      "eslint --fix",
      "git add *"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
