    "css-loader": "^2.1.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^2.0.0",
